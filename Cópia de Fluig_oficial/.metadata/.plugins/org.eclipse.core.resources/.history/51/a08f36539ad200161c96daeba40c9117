} 

function permissao(){

 

  var logadoCaixa = "${pageRender.user.userCode}";

  var constrait1 = DatasetFactory.createConstraint("usuario", logadoCaixa, logadoCaixa, ConstraintType.SHOULD);
  var constraints   = new Array(constrait1);
  var fields = null;
  var sortingFields = null;  
  var CaixaBerrini;

  try {

    var dataset = DatasetFactory.getDataset("GH_Permissionamento", fields, constraints, sortingFields);

    for(i = 0; i < dataset.values.length; i++) {


      var row = dataset.values[i];


      CaixaBerrini = row["Berrini"];
      CaixaPaulista = row["Paulista"];
      CaixaBonno = row["Bonno"];
      CaixaRecord = row["Record"];
      CaixaPenha = row["Penha"];
      CaixaMorumbi = row['Morumbi'];


      if (CaixaBerrini == "True") {
        $(".berrini").css('display','inline-block');
      }

      if (CaixaPaulista == "True") {
        $(".paulista").css('display','inline-block');
      }

      if (CaixaBonno == "True") {
        $(".bonno").css('display','inline-block');
      }

      if (CaixaRecord == "True") {
        $(".record").css('display','inline-block');
      }

      if (CaixaPenha == "True") {

        $(".penha").css('display','inline-block');
      }

      if (CaixaMorumbi == "True") {

        $(".morumbi").css('display','inline-block');
      } 



    }

  } catch(erro) {
    alert(erro);
  } 


}











</script>


