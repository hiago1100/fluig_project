textoAux4 += "<font>"+documentoAux+"</font></td>";                    

if (tipoAux == "entrada" || tipoAux == "saida") {

 textoAux4 += "<td class=\"td\" align=\"center\">";      
 textoAux4 += "<font>"+tipoAux+"</font></td>"; 
} else if (tipoAux == "ENT") {

 textoAux4 += "<td class=\"td\" align=\"center\">";      
 textoAux4 += "<font>entrada</font></td>";

} else if (tipoAux == "SAI") {

 textoAux4 += "<td class=\"td\" align=\"center\">";      
 textoAux4 += "<font>saida</font></td>";

}

textoAux4 += "<td class=\"td\" align=\"center\">";;
if(tipoAux ==  "entrada" || tipoAux == "ENT") {
 textoAux4 += "<font style=\"color:green;\">";
}else{

 textoAux4 += "<font style=\"color:red;\">";
} 
textoAux4 += "" +valorMovto+ "";   

textoAux4 += "<font\>";


}  } 
recorrentePenha = vlTotalPenhaAnt + vlTotalPenhaAtual;

break;     

  // IMPLANTAÇÃO MORUMBI

  case "WJ Bonno Morumbi":


  if (dataLancamentoAux != null) {
   var dataSplit = dataLancamentoAux.split("/"); 

   var mesLancamento =  dataSplit[1] - 1;

// Saldo do mês atual.
if (mesAtual == mesLancamento) {
  vlTotalEnMorumbi  = vlTotalEnMorumbi  +  vlTotalEntAux;
  vlTotalSaiMorumbi = vlTotalSaiMorumbi +  vlTotalSaiAux;  
  vlTotalMorumbi    = vlTotalEnMorumbi  -  vlTotalSaiMorumbi;
}

if (mesLancamento < mes5){

  vlTotalEnMorumbiAnt  = vlTotalEnMorumbiAnt  +  vlTotalEntAux;
  vlTotalsaiMorumbiAnt = vlTotalsaiMorumbiAnt +  vlTotalSaiAux;  
  vlTotalMorumbiAnt    = vlTotalEnMorumbiAnt  -  vlTotalsaiMorumbiAnt;                              
}

if (mes5 == mesLancamento) {


  vlTotalEnMorumbiAtual  = vlTotalEnMorumbiAtual  +  vlTotalEntAux;
  vlTotalSaiMorumbiAtual = vlTotalSaiMorumbiAtual +  vlTotalSaiAux;  
  vlTotalMorumbiAtual    = vlTotalEnMorumbiAtual  -  vlTotalSaiMorumbiAtual;
}







if (mes5 == mesLancamento && dataSplit[2] == itemSelecionado) {  







  linkSoclicitacao = '<a href="http://viaindicadores.fluig.com:8280/portal/p/1/pageworkflowview?app_ecm_workflowview_detailsProcessInstanceID='+solicitacaoAux+'" target="_blank">'+solicitacaoAux+'</a>';

  textoAux5 += "<tr>";

  textoAux5 += "<td class=\"td\" align=\"center\">";      
  textoAux5 += "<font>"+dataLancamentoAux+"</font></td>";

  textoAux5 += "<td class=\"td\" align=\"center\">";      
  textoAux5 += "<font>"+linkSoclicitacao+"</font></td>";

  textoAux5 += "<td class=\"td\">";      
  textoAux5 += "<font>"+emitenteAux+"</font></td>";

  if (tipoAux == "entrada"){

   textoAux5 += "<td class=\"td\" align=\"center\">";  

   textoAux5 += "<font>"+entradaAux+"</font></td>"; 


 } else if (tipoAux == "saida"){

   textoAux5 += "<td class=\"td\" align=\"center\">";  

   textoAux5 += "<font>"+saidaAux+"</font></td>"; 

 }else if ( tipoAux =="ENT" || tipoAux == "SAI") {

  textoAux5 += "<td class=\"td\" align=\"center\">";  

  textoAux5 += "<font>"+tipoEntradaAux+"</font></td>"; 


}

textoAux5 += "<td class=\"td\" align=\"center\">";      
textoAux5 += "<font>"+documentoAux+"</font></td>";                    

if (tipoAux == "entrada" || tipoAux == "saida") {

 textoAux5 += "<td class=\"td\" align=\"center\">";      
 textoAux5 += "<font>"+tipoAux+"</font></td>"; 
} else if (tipoAux == "ENT") {

 textoAux5 += "<td class=\"td\" align=\"center\">";      
 textoAux5 += "<font>entrada</font></td>";

} else if (tipoAux == "SAI") {

 textoAux5 += "<td class=\"td\" align=\"center\">";      
 textoAux5 += "<font>saida</font></td>";

}

textoAux5 += "<td class=\"td\" align=\"center\">";;
if(tipoAux ==  "entrada" || tipoAux == "ENT") {
 textoAux5 += "<font style=\"color:green;\">";
}else{

 textoAux5 += "<font style=\"color:red;\">";
} 
textoAux5 += "" +valorMovto+ "";   

textoAux5 += "<font\>";


}  } 
recorrenteMorumbi = vlTotalMorumbiAnt + vlTotalMorumbiAtual;


default:
break;

}




}
} catch(erro) {
  alert(erro);
} 






$("#tableResumo").html(textoAux);
$("#tableResumo1").html(textoAux1);
$("#tableResumo2").html(textoAux2);
$("#tableResumo3").html(textoAux3);
$("#tableResumo4").html(textoAux4);
$("#tableResumo5").html(textoAux5);


$("#totalMes").html("Total:  R$" + recorrenteBerrini.toFixed(2) );
$("#totalMes1").html("Total: R$" + recorrentePaulista.toFixed(2) );
$("#totalMes2").html("Total: R$" + recorrenteBonno.toFixed(2) );
$("#totalMes3").html("Total: R$" + recorrenteRecord.toFixed(2) );
$("#totalMes4").html("Total: R$" + recorrentePenha.toFixed(2) );
$("#totalMes5").html("Total: R$" + recorrenteMorumbi.toFixed(2) );


if (tipo == "onLoad") {  


  if(recorrenteBerrini >= 0){
    $("#vlTotalBerrini").attr("style", "color:green");
    $("#totalMes").attr("style", "color:green;margin-right:90px");
  }else{
    $("#vlTotalBerrini").attr("style", "color:red");
    $("#totalMes").attr("style", "color:red;margin-right:90px");
  }

  if(recorrentePaulista >= 0){
    $("#vlTotalPaulista").attr("style", "color:green");
    $("#totalMes1").attr("style", "color:green;margin-right:90px");
  }else{
    $("#vlTotalPaulista").attr("style", "color:red");
    $("#totalMes1").attr("style", "color:red;margin-right:90px");
  }
  if(recorrenteBonno >= 0){
    $("#vlTotalBonno").attr("style", "color:green");
    $("#totalMes2").attr("style", "color:green;margin-right:90px");
  }else{
    $("#vlTotalBonno").attr("style", "color:red");
    $("#totalMes2").attr("style", "color:red;margin-right:90px");
  }
  if(recorrenteRecord >= 0){
    $("#vlTotalRecord").attr("style", "color:green");
    $("#totalMes3").attr("style", "color:green;margin-right:90px");
  }else{
    $("#vlTotalRecord").attr("style", "color:red");
    $("#totalMes3").attr("style", "color:red;margin-right:90px");
  }
  if(recorrentePenha >= 0){

    $("#vlTotalPenha").attr("style", "color:green");
    $("#totalMes4").attr("style", "color:green;margin-right:90px");
  }else{
    $("#vlTotalPenha").attr("style", "color:red");
    $("#totalMes4").attr("style", "color:red;margin-right:90px");
  }

  if(recorrenteMorumbi >= 0){
    $("#vlTotalMorumbi").attr("style", "color:green");
    $("#totalMes5").attr("style", "color:green;margin-right:90px");
  }else{
    $("#vlTotalMorumbi").attr("style", "color:red");
    $("#totalMes5").attr("style", "color:red;margin-right:90px");
  }


  $("#vlTotalBerrini").html("<div class=\"panel panel-default\"> <div class=\"panel-heading\" align=\"center\">Berrini </div> R$ " + recorrenteBerrini.toFixed(2) + "</div>" );

  $("#vlTotalPaulista").html("<div class=\"panel panel-default\"><div class=\"panel-heading\" align=\"center\">Paulista </div> R$ " + recorrentePaulista.toFixed(2) + "</div>");
  $("#vlTotalBonno").html("<div class=\"panel panel-default\"> <div class=\"panel-heading\" align=\"center\">Bonno Corifeu </div> R$ " +recorrenteBonno.toFixed(2) + "</div> " );
  $("#vlTotalRecord").html("<div class=\"panel panel-default\"> <div class=\"panel-heading\" align=\"center\">Record </div> R$ " + recorrenteRecord.toFixed(2) + "</div>") ;
  $("#vlTotalPenha").html("<div class=\"panel panel-default\"> <div class=\"panel-heading\" align=\"center\">Penha </div> R$ " +recorrentePenha.toFixed(2) + "</div>");
  $("#vlTotalMorumbi").html("<div class=\"panel panel-default\"> <div class=\"panel-heading\" align=\"center\">Bonno Morumbi </div> R$ " +recorrenteMorumbi.toFixed(2) + "</div>");

}             


$("#AtualBerrini").html("Anterior: R$ " + vlTotalBerriniAnt.toFixed(2));
